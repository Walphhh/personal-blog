{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/ncc",
      "config": {
        "targets": ["dist"]
      }
    },
    {
      "src": "dist/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/dist/[1].js"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/server.js"
    }
  ]
}

{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/ncc",
      "config": {
        "targets": ["dist"]
      }
    },
    {
      "src": "dist/api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/dist/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/api/index.js"
    }
  ]
}
